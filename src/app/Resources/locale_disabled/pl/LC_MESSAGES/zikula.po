msgid_plural "Do you really want to delete these user accounts?"
msgstr[0] "Czy na pewno chcesz usunąć tego użytkownika?"
msgstr[1] "Czy na pewno chcesz usunąć tych użytkowników?"
msgstr[2] "Czy na pewno chcesz usunąć tych użytkowników?"

#: system/Zikula/Module/UsersModule/Resources/views/Admin/toggleforcedpasswordchange.tpl:4
#, php-format
msgid "Cancel forced password change for %1$s"
msgstr "Anulowanie żądania zmiany hasła dla konta %1$s"

#: system/Zikula/Module/UsersModule/Resources/views/Admin/toggleforcedpasswordchange.tpl:6
#, php-format
msgid "Force a change of password for %1$s"
msgstr "Wymuszenie zmiany hasła dla konta %1$s"

#: system/Zikula/Module/UsersModule/Resources/views/Admin/toggleforcedpasswordchange.tpl:15
#, php-format
msgid "Do you want to cancel the forced password change for %1$s?"
msgstr "Czy na pewno chcesz anulować żądanie zmiany hasła dla konta %1$s?"

#: system/Zikula/Module/UsersModule/Resources/views/Admin/toggleforcedpasswordchange.tpl:17
#, php-format
